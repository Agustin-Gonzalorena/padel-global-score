---
export const prerender = true;

import Layout from "../layouts/Layout.astro";
import FormLogin from "../components/react/FormLogin.jsx";
const BASE_URL = import.meta.env.FINAL_BASE_URL;
---

<Layout>
  <div class="flex flex-col text-center w-full p-5">
    <h1 class="sm:text-3xl text-2xl font-medium title-font">Iniciar Sesion</h1>
    <p class="text-sm text-white/70">Rappa no podes hackear esto</p>
  </div>
  <FormLogin client:load BASE_URL={BASE_URL} />
</Layout>
